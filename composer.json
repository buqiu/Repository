{
    "name": "buqiu/repository",
    "description": "Laravel Repository 是 Laravel 的一个软件包，用于抽象数据库层。 这使应用程序易于维护。",
    "license": "MIT",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "repositories",
        "repository",
        "eloquent",
        "database"
    ],
    "authors": [
        {
            "name": "董坤鸿",
            "email": "249529979@qq.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "Buqiu\\Repository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Buqiu\\Repository\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Buqiu\\Repository\\Providers\\RepositoryProvider"
            ]
        }
    }
}
